{
	"name": "WikibaseLexeme",
	"private": true,
	"config": {
		"remoteVersion": {
			"vue": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vue/vue.common.prod.js",
			"vuex": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vuex/vuex.js"
		},
		"snl": {
			"src": "resources/special/new-lexeme",
			"dist": "resources/special/new-lexeme-dist",
			"cjs": "SpecialNewLexeme.cjs.js",
			"css": "style.css"
		}
	},
	"scripts": {
		"selenium-daily": "MEDIAWIKI_USER='Selenium user' MW_SERVER=https://wikidata.beta.wmflabs.org:443 MW_SCRIPT_PATH=/w WIKIBASE_PROPERTY_STRING=P443 npm run selenium-test",
		"selenium-test": "wdio tests/selenium/wdio.conf.js",
		"test": "run-s test:*",
		"test:grunt": "grunt test",
		"test:snl-distnodiff": "run-s bsnl:install bsnl:build bsnl:cp bsnl:diff",
		"test:snl-main": "git -C $npm_package_config_snl_src branch --contains HEAD main | grep -q .",
		"test:mwlibs": "echo 'disabled (T297381)' # ZUUL_BRANCH=${ZUUL_BRANCH:-master} lib-version-check",
		"fix": "grunt fix",
		"bump-special-new-lexeme": "run-s bsnl:fetch bsnl:checkout bsnl:install bsnl:build bsnl:cp bsnl:add bsnl:restore",
		"bsnl:fetch": "git -C $npm_package_config_snl_src fetch origin main",
		"bsnl:checkout": "git -C $npm_package_config_snl_src -c advice.detachedHead=false checkout FETCH_HEAD",
		"bsnl:install": "npm -C $npm_package_config_snl_src i",
		"bsnl:build": "npm -C $npm_package_config_snl_src run build",
		"bsnl:cp": "run-p bsnl:cp:*",
		"bsnl:cp:cjs": "cp $npm_package_config_snl_src/dist/$npm_package_config_snl_cjs $npm_package_config_snl_dist/",
		"bsnl:cp:css": "cp $npm_package_config_snl_src/dist/$npm_package_config_snl_css $npm_package_config_snl_dist/",
		"bsnl:add": "git add $npm_package_config_snl_src $npm_package_config_snl_dist",
		"bsnl:restore": "git -C $npm_package_config_snl_src checkout -",
		"bsnl:diff": "git diff --exit-code $npm_package_config_snl_dist"
	},
	"devDependencies": {
		"@wdio/cli": "^7.16.14",
		"@wdio/dot-reporter": "^7.16.14",
		"@wdio/junit-reporter": "^7.16.14",
		"@wdio/local-runner": "^7.16.16",
		"@wdio/mocha-framework": "^7.16.14",
		"@wdio/sync": "^7.16.14",
		"@wmde/lib-version-check": "^0.1.0",
		"eslint-config-wikimedia": "0.20.0",
		"eslint-plugin-promise": "^6.0.0",
		"grunt": "1.5.3",
		"grunt-banana-checker": "0.9.0",
		"grunt-eslint": "24.0.0",
		"grunt-jasmine-nodejs": "^1.6.1",
		"grunt-stylelint": "0.16.0",
		"jasmine": "^4.0.2",
		"jquery": "^3.6.0",
		"jsdom": "^19.0.0",
		"jsdom-global": "^3.0.2",
		"lodash": "^4.17.21",
		"module-alias": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"request": "^2.88.2",
		"sinon": "^13.0.0",
		"stylelint-config-wikimedia": "0.11.1",
		"unexpected": "^10.32.1",
		"unexpected-dom": "^5.0.1",
		"unexpected-sinon": "^11.0.1",
		"vue": "^2.6.11",
		"vuex": "^3.1.3",
		"wdio-mediawiki": "^1.2.0",
		"wdio-wikibase": "^5.2.0",
		"webdriverio": "^7.16.14"
	},
	"browserslist": [
		"ie 11"
	]
}
